void main()
{
    $debug_break();
    
    u16[2][3] a = { {1, 2}, {3, 4}, {5, 6} };
    
    {
        u16[]& r = a;
        $assert(len(r) == 6);
        $assert(r[0] == 1);
        $assert(r[1] == 2);
        $assert(r[2] == 3);
        $assert(r[3] == 4);
        $assert(r[4] == 5);
        $assert(r[5] == 6);
    }
    
    
    {
        u16[]& r = a[1:3];
        $assert(len(r) == 4);
        $assert(r[0] == 3);
        $assert(r[1] == 4);
        $assert(r[2] == 5);
        $assert(r[3] == 6);
    }

    $debug_break();
}
